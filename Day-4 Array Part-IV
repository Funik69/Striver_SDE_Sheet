int Solution::solve(vector<int> &A, int B) {
    unordered_map<int,int>visited;
    int x = 0;
    long long cnt=0;
    for(int i=0;i<A.size();i++){
        x^=A[i];
        if(x==B) cnt++;
        int h = x^B;
        if(visited.find(h)!=visited.end()){
            cnt=cnt+visited[h];
        }
        visited[x]++;
    }
    return cnt;
}

